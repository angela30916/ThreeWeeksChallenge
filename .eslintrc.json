{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint-config-prettier", "plugin:react/recommended", "airbnb"],
  "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {}
}
